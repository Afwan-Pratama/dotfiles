# Main background, empty for terminal default, need to be empty if you want transparent background
theme[main_bg]=

# Main text color
# {#replace_color(data.colors.base05)#}
theme[main_fg]="#dcd7ba"

# Title color for boxes
# {#replace_color(data.colors.base05)#}
theme[title]="#dcd7ba"

# Highlight color for keyboard shortcuts
# {#replace_color(data.colors.base0D)#}
theme[hi_fg]="#7e9cd8"

# Background color of selected item in processes box
# {#replace_color(data.colors.base01)#}
theme[selected_bg]="#16161d"

# Foreground color of selected item in processes box
# {#replace_color(data.colors.base0C)#}
theme[selected_fg]="#6a9589"

# Color of inactive/disabled text
# {#replace_color(data.colors.base03)#}
theme[inactive_fg]="#54546d"

# Color of text appearing on top of graphs, i.e uptime and current network graph scaling
# {#replace_color(data.colors.base04)#}
theme[graph_text]="#727169"

# Background color of the percentage meters
# {#replace_color(data.colors.base01)#}
theme[meter_bg]="#16161d"

# Misc colors for processes box including mini cpu graphs, details memory graph and details status text
# {#replace_color(data.colors.base04)#}
theme[proc_misc]="#727169"

# CPU, Memory, Network, Proc box outline colors
# {#replace_color(data.colors.base0E)#}
theme[cpu_box]="#957fb8" #Mauve
# {#replace_color(data.colors.base0B)#}
theme[mem_box]="#76946a" #Green
# {#replace_color(data.colors.base0F)#}
theme[net_box]="#d27e99" #Maroon
# {#replace_color(data.colors.base0C)#}
theme[proc_box]="#6a9589" #Blue

# Box divider line and small boxes line color
# {#replace_color(data.colors.base02)#}
theme[div_line]="#223249"

# Temperature graph color (Green -> Yellow -> Red)
# {#replace_color(data.colors.base0B)#}
theme[temp_start]="#76946a"
# {#replace_color(data.colors.base0A)#}
theme[temp_mid]="#c0a36e"
# {#replace_color(data.colors.base08)#}
theme[temp_end]="#c34043"

# CPU graph colors (Teal -> Lavender)
# {#replace_color(data.colors.base0C)#}
theme[cpu_start]="#6a9589"
# {#replace_color(data.colors.base0D)#}
theme[cpu_mid]="#7e9cd8"
# {#replace_color(data.colors.base0E)#}
theme[cpu_end]="#957fb8"

# Mem/Disk free meter (Mauve -> Lavender -> Blue)
# {#replace_color(data.colors.base0E)#}
theme[free_start]="#957fb8"
# {#replace_color(data.colors.base0D)#}
theme[free_mid]="#7e9cd8"
# {#replace_color(data.colors.base0C)#}
theme[free_end]="#6a9589"

# Mem/Disk cached meter (Sapphire -> Lavender)
# {#replace_color(data.colors.base0C)#}
theme[cached_start]="#6a9589"
# {#replace_color(data.colors.base0D)#}
theme[cached_mid]="#7e9cd8"
# {#replace_color(data.colors.base0E)#}
theme[cached_end]="#957fb8"

