theme[available_start]="#ffa066"
# {#replace_color(data.colors.base0F)#}
theme[available_mid]="#d27e99"
# {#replace_color(data.colors.base08)#}
theme[available_end]="#c34043"

# Mem/Disk used meter (Green -> Sky)
